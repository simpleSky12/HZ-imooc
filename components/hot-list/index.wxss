@import "../../wxss/sleeve.wxss";
.container{
    display:flex;
    flex-direction:column;
    align-items: center;
    background-color: #fff;
    padding-top: 40rpx;
}
.title{
    width: 690rpx;
    height: 90rpx;
}
.inner-container{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-top: 30rpx;
}
.left-img{
    display: flex;
    width: 346rpx;
    height: 522rpx;
}
.right-container{
    display:flex;
    flex-direction:column;
    justify-content: space-between;
}
.right-img{
    display: flex;
    width: 400rpx;
    height: 260rpx;
}
